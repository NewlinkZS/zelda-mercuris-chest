-- A shader that does nothing special.

videomode {
	name = "default",
	default_window_scale = 2,

	-- Source of the fragment shader.
	fragment_source = [[

    #version 120

    uniform sampler2D quest_texture;

    void main() {
      gl_FragColor = texture2D(quest_texture, gl_TexCoord[0].xy);
    }
	]]
}
